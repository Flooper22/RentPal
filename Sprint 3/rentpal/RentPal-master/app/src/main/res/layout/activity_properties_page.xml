<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/propertiespage_bg"
    tools:context=".PropertiesPage">


    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="294sp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="65sp"
        android:layout_marginTop="100sp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Search for Property Name"
        android:textColor="#000000"
        android:textSize="10sp" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="19sp"
        android:layout_height="35sp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="44sp"
        android:layout_marginTop="100sp"
        app:srcCompat="@drawable/searchicon" />

    <TextView
        android:id="@+id/historytitle3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerInParent="true"
        android:layout_marginTop="30sp"
        android:layout_marginEnd="158sp"
        android:text="Properties"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/backbtnproperties"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="19sp"
        android:layout_marginTop="18sp"
        android:onClick="onClick"
        app:srcCompat="@drawable/backbutton" />

    <ImageView
        android:id="@+id/quickmenubtnproperties"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="24sp"
        android:layout_marginEnd="20sp"
        app:srcCompat="@drawable/quickmenuicon" />
</RelativeLayout>