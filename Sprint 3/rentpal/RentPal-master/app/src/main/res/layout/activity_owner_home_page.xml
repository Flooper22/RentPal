<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/hometenant_bg"
    tools:context=".OwnerHomePage">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="119dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="150dp"
        android:layout_marginTop="431dp"
        android:text="Latest Post"
        android:textColor="#143656"
        android:textSize="20sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView30"
        android:layout_width="77sp"
        android:layout_height="109sp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerInParent="true"
        android:layout_marginEnd="30sp"
        android:layout_marginBottom="273sp"
        app:srcCompat="@drawable/propertyicon" />

    <Button
        android:id="@+id/propertiesbtn"
        android:layout_width="wrap_content"
        android:layout_height="36sp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="376sp"
        android:layout_marginEnd="24sp"
        android:text="Properties"
        android:textColor="#000000"
        android:textSize="8sp"
        app:backgroundTint="#D9D9D9" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="238sp"
        android:layout_height="176sp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="109sp"
        android:layout_marginEnd="36sp"
        app:srcCompat="@drawable/profileholder" />

    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="77sp"
        android:layout_height="109sp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="142sp"
        android:layout_marginTop="140sp"
        app:srcCompat="@drawable/homeprofile" />

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="43sp"
        android:layout_height="44sp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="40sp"
        android:layout_marginEnd="81sp"
        app:srcCompat="@drawable/notificationicon" />

    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="43sp"
        android:layout_height="44sp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="40sp"
        android:layout_marginEnd="22sp"
        app:srcCompat="@drawable/settingsicon" />

    <TextView
        android:id="@+id/HomeTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="22sp"
        android:layout_marginEnd="175sp"
        android:text="HOME"
        android:textColor="#000000"
        android:textSize="60sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="77sp"
        android:layout_height="109sp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="30sp"
        android:layout_marginBottom="274sp"
        app:srcCompat="@drawable/detailsicon" />

    <Button
        android:id="@+id/rentdetailsbtn"
        android:layout_width="wrap_content"
        android:layout_height="36sp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="375sp"
        android:layout_marginEnd="290sp"
        android:text="Rental Details"
        android:textColor="#000000"
        android:textSize="8sp"
        app:backgroundTint="#D9D9D9" />

    <ImageView
        android:id="@+id/imageView31"
        android:layout_width="75sp"
        android:layout_height="82sp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerInParent="true"
        android:layout_marginEnd="164sp"
        android:layout_marginBottom="285sp"
        app:srcCompat="@drawable/reporticon" />

    <Button
        android:id="@+id/reportbtn"
        android:layout_width="wrap_content"
        android:layout_height="36sp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="375sp"
        android:layout_marginEnd="146sp"
        android:text="Income Report"
        android:textColor="#000000"
        android:textSize="8sp"
        app:backgroundTint="#D9D9D9" />

</RelativeLayout>